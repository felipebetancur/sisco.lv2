sisco:Mono@URI_SUFFIX@
	a lv2:Plugin, lv2:AnalyserPlugin ;
	doap:name "Simple Scope (Mono)@NAME_SUFFIX@" ;
	lv2:project <http://gareus.org/oss/lv2/sisco> ;
	doap:license <http://usefulinc.com/doap/licenses/gpl> ;
	lv2:requiredFeature urid:map ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:extensionData state:interface ;
	ui:ui sisco:@SISCOUI@ ;
	lv2:port [
		a atom:AtomPort ,
			lv2:InputPort ;
		atom:bufferType atom:Sequence ;
		lv2:designation lv2:control ;
		lv2:index 0 ;
		lv2:symbol "control" ;
		lv2:name "Control"
	] , [
		a atom:AtomPort ,
			lv2:OutputPort ;
		atom:bufferType atom:Sequence ;
		lv2:designation lv2:control ;
		lv2:index 1 ;
		lv2:symbol "notify" ;
		lv2:name "Notify" ;
		# 8192 * sizeof(float) + LV2-Atoms
		rsz:minimumSize 33024;
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 2 ;
		lv2:symbol "in" ;
		lv2:name "In"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 3 ;
		lv2:symbol "out" ;
		lv2:name "Out"
	] ;
	rdfs:comment "Single channel audio oscilloscope with variable time scale, triggering, markers and numeric readout."
	.


